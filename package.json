{
  "name": "modelviewertest",
  "version": "1.2.0",
  "description": "Model Viewer Test",
  "author": "EuroSzymon",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist":  "electron-builder"
  },
  "build": {
    "appId": "com.euroszymon.modelviewertest",
    "directories": {
      "output": "release"
    },
    "files": [
      "main.js",
      "index.html",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "assets/viewer!.ico",
      "target": [
        "portable"
      ]
    },
    "linux": {
      "icon": "assets/viewer!.png",
      "target": "AppImage",
      "category": "Graphics" 
    }
  },
  "devDependencies": {
    "electron": "^36.2.0",
    "electron-builder": "^24.0.0"
  }
}
